{
  "name": "es-git",
  "version": "0.4.0",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "bugs": "https://github.com/toss/es-git/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/toss/es-git.git"
  },
  "license": "MIT",
  "packageManager": "yarn@4.9.2",
  "napi": {
    "name": "es-git",
    "triples": {
      "defaults": false,
      "additional": [
        "x86_64-apple-darwin",
        "aarch64-apple-darwin",
        "x86_64-pc-windows-msvc",
        "aarch64-pc-windows-msvc",
        "x86_64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "armv7-linux-androideabi"
      ]
    }
  },
  "engines": {
    "node": ">= 10.20.0"
  },
  "workspaces": [
    "docs",
    "benchmarks"
  ],
  "scripts": {
    "build": "napi build --platform --release --no-const-enum --pipe=\"yarn transform:dts\"",
    "build:debug": "DEBUG=\"napi:*\" napi build --platform --no-const-enum --pipe=\"yarn transform:dts\"",
    "transform:dts": "jscodeshift -t transforms/dts.mjs index.d.ts",
    "check": "biome check",
    "check:fix": "biome check --write --unsafe"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@napi-rs/cli": "^2.18.4",
    "@types/jscodeshift": "^0.12.0",
    "@types/node": "^22.8.4",
    "fast-glob": "^3.3.3",
    "jscodeshift": "^17.1.2",
    "typescript": "5.8.2",
    "vitest": "^3.0.5"
  }
}
